{
  "name": "wework-china-trim",
  "version": "1.0.0",
  "productName": "WeWork Trim",
  "description": "WeWork Trim",
  "main": "dist/main/app.bundle.js",
  "scripts": {
    "package": "electron-builder build",
    "build-main": "webpack --config ./main/webpack.config.js",
    "build-renderer": "webpack --config ./renderer/webpack.config.js",
    "desktop": "electron ./dist/main/app.bundle.js",
    "start": "export ENV=development && npm run build-main && npm run build-renderer && npm run desktop"
  },
  "author": "WeWork",
  "license": "WeWork",
  "build": {
    "productName": "WeWork Trim",
    "appId": "com.wework.trim",
    "copyright": "WeWork LLC",
    "directories": {
      "output": "builds"
    },
    "asar": "true",
    "files": [
      "./dist/**/*",
      "!node_modules/**/*",
      "!**/*.d.ts"
    ],
    "icon": "resources/Tars.icns"
  },
  "pkg": {
    "installLocation": "/Applications"
  },
  "dmg": {
    "contents": [
      {
        "x": 410,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 130,
        "y": 150,
        "type": "file"
      }
    ]
  },
  "dependencies": {
    "@weconnect/appkit": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "electron-store": "^5.1.1",
    "react-redux": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "electron": "8.3.0",
    "electron-builder": "^22.6.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^4.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.10.3",
    "zip-webpack-plugin": "^3.0.0"
  }
}
